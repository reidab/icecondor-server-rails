<% content_for :header do %>
  <script src="http://maps.google.com/maps/api/js?sensor=false&libraries=geometry" type="text/javascript"></script>
  <%= javascript_include_tag "geopoly.js" %>
<% end %>


<h3>Edit a GeoFence. </h3>
<% centerpoint = @fence.centerpoint %>
<%= render :partial => "gmap_edit", :locals => {:fence => @fence,
                                                :geojson => @fence.geom.to_geojson,
                                                :centerpoint => [@fence.centerpoint[:latitude],
                                                                 @fence.centerpoint[:longitude]]} %>

